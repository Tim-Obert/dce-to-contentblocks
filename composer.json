    "type": "typo3-cms-extension",
    "title": "DCE to Content Blocks",
    "authors": [
        {
            "name": "Thorben Nissen",
            "email": "thorben@webcoast.dk"
        }
    ],
    "license": "GPL-3.0-or-later",
    "extra": {
